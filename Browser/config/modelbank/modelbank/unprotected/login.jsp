<%
response.setHeader("Cache-Control","no-cache,no-store");
response.setHeader("Pragma","no-cache");
response.setDateHeader ("Expires", 0);
%>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
	<title>T24 Sign On</title>

	<style type="text/css">
		.instruction {
			font-family: Arial, Times, serif;
			font-weight: bold;
		}
		.forgotten {
			font-family: Arial, Courier, monospace;
			font-size: small;
			font-weight: normal;
			color: #FF0000;
		}
		.reminder {
			font-family: Arial, Times, serif;
			font-size: small;
			font-weight: bold;
			color: #666666;
		}
		body {
			background-color: #D4D4D4;
		}
		table.input {
			border:1px solid black; 
			width:50%;
			padding: 10px;
		}		
	</style>


	<script LANGUAGE="javascript">
		//<![CDATA[
			function setFocus() {
				window.document.forms["login"].j_username.focus();
			}

			function doLogin() {
				var loginForm = window.document.forms["login"];
				var otp = loginForm.password.value;
				var pin = loginForm.pin.value;
				loginForm.j_password.value = otp + "|" +	 pin;
				loginForm.submit();
			}
		//]]>
	</script>
</head>

<body onload="setFocus()">
	<table border="0" width="994">
		<tbody><tr><td><p>
		<img src="<%=request.getContextPath()%>/modelbank/unprotected/yourbank.jpg" usemap="#Map" border="0">
		<map name="Map" id="Map">
			<area shape="rect" coords="691,4,737,24" href='javascript:alert("logoff")'>
			<area shape="rect" coords="739,4,773,23" href='javascript:alert("help")'>
			<area shape="rect" coords="776,4,857,22" href='javascript:alert("accessibility")'>
			<area shape="rect" coords="862,3,920,22" href='javascript:alert("sitemap")'>
			<area shape="rect" coords="922,2,962,23" href='javascript:alert("legal")'>
			<area shape="rect" coords="964,3,1029,21" href='javascript:alert("privacy")'>
		</map>

		</p><p><p style="font-weight: bold;color:#ff0000"/><p class="instruction">
    Not registered yet? Please register <a href="<%=request.getContextPath()%>/modelbank/unprotected/bind.jsp">here</a></p>

		<form name="login" method="post" action="j_security_check" AUTOCOMPLETE="OFF">

		<input name="j_password" value="" type="hidden"/>

		<table class="input">
			<tr>
				<td colspan="2" class="instruction">
					Enter your User ID
				</td>
			</tr>
			<tr>
				<td width="23%">
					<input name="j_username" size="40" type="password" value=""/>
				</td>
				<td class="forgotten" width="77%">
					Forgotton your User ID?
				</td>
			</tr>
			<tr>
				<td colspan="2" class="reminder">
					(This is your seven character ID for online and phone banking services)
				</td>
			</tr>
			<tr>
				<td colspan="2">
				</td>
			</tr>
			<tr>
				<td colspan="2" class="instruction">
					Enter your one-time password
				</td>
			</tr>
			<tr>
				<td>
					<input name="password" size="40" type="password"/>
				</td>
				<td class="forgotten">
					Lost or broken your token device?
				</td>
			</tr>
			<tr>
				<td colspan="2" class="reminder">
					(This is the six digit number generated by your token device)
				</td>
			</tr>
			<tr>
				<td colspan="2">
				</td>
			</tr>
			<tr>
				<td colspan="2" class="instruction">
					Finally, please enter your online banking PIN
				</td>
			</tr>
			<tr>
				<td>
					<input name="pin" size="40" type="password" value=""/>
				</td>
				<td class="forgotten">
					Forgotton your PIN?
				</td>
			</tr>
			<tr>
				<td colspan="2" class="reminder">
					(This is the PIN number issued to you by your bank)
				</td>
			</tr>
		</table>
	<p>
		<a href="javascript:doLogin()">
			<img src="<%=request.getContextPath()%>/modelbank/unprotected/login.gif" border="0"/>
		</a>
	</p>

</form>
</td></tr><tr><td align="right"><img src="<%=request.getContextPath()%>/modelbank/unprotected/double%20eye.gif"></td></tr></tbody></table>
</body>
</html>
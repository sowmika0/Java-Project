   ldap.server.url="ldap://[LDAP_HOSTNAME]:[LDAP_PORT]/"
   ca.rootdn="[LDAP_ROOT_CA_CRL]"
   ca.trustedAlias="[LDAP_CA_CRL_TRUSTED]"; 
};


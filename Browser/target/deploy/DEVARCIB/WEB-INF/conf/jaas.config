
login {
   com.temenos.tocf.security.Authent.module.TClientLDAPLoginModule required debug=false
   ldap.security.credential.type = "simple"
   user.provider.url="ldap://[LDAP_HOSTNAME]:[LDAP_PORT]/[CORPORATE_ROOT_DN]"
   ldap.security.credential.type = "simple"   
   user.provider.login="[LDAP_LOGIN]"
   user.provider.password="[LDAP_PASSWORD]"
